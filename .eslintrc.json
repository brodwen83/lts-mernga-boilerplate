{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": 0,
    "space-in-parens": ["error", "never"],
    "no-underscore-dangle": "off",
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
